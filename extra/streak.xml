<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="streak"
           fetch="https://github.com/ProjectStreak"
           revision="refs/heads/twelve" />

  <remote  name="streak-sc"
           fetch="ssh://git@github.com/ProjectStreak-SC"
           revision="refs/heads/twelve" />

  <!-- rootdir -->
  <project path="manifest" name="platform_manifest" remote="streak-sc" />
  <project path="art" name="platform_art" remote="streak-sc" />
  <project path="bionic" name="platform_bionic" remote="streak-sc" />
  <project path="libcore" name="platform_libcore" remote="streak-sc" />

  <!-- build -->
  <project path="build/make" name="platform_build" remote="streak-sc" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="platform_build_soong" groups="pdk,tradefed" remote="streak-sc" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <project path="bootable/recovery" name="platform_bootable_recovery" remote="streak-sc" />

  <!-- device -->
  <project path="device/qcom/sepolicy" name="platform_device_qcom_sepolicy" remote="streak-sc" /> 
  <project path="device/qcom/sepolicy_vndr" name="platform_device_qcom_sepolicy_vndr" remote="streak-sc" /> 
  <project path="device/qcom/sepolicy-legacy" name="platform_device_qcom_sepolicy" remote="streak-sc" revision="twelve-legacy" />
  <project path="device/qcom/sepolicy-legacy-um" name="platform_device_qcom_sepolicy" remote="streak-sc" revision="twelve-legacy-um" />
  <project path="device/streak/sepolicy" name="platform_device_streak_sepolicy" remote="streak-sc" /> 

  <!-- external -->
  <project path="external/e2fsprogs" name="platform_external_e2fsprogs" remote="streak-sc" />
  <project path="external/gptfdisk" name="platform_external_gptfdisk" remote="streak-sc" />
  <project path="external/mksh" name="platform_external_mksh" remote="streak-sc" />
  <project path="external/selinux" name="platform_external_selinux" remote="streak-sc"  /> 
  <project path="external/tinycompress" name="platform_external_tinycompress" remote="streak-sc"  />

  <!-- frameworks -->
  <project path="frameworks/av" name="platform_frameworks_av" remote="streak-sc" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="streak-sc" />
  <project path="frameworks/native" name="platform_frameworks_native" remote="streak-sc" />

  <!-- hardware -->
  <project path="hardware/interfaces" name="platform_hardware_interfaces" remote="streak-sc" />
  <project path="hardware/libhardware" name="platform_hardware_libhardware" remote="streak-sc" />
  <project path="hardware/libhardware_legacy" name="platform_hardware_libhardware_legacy" remote="streak-sc" />
  <project path="hardware/ril" name="platform_hardware_ril" groups="pdk" remote="streak-sc" />
  <project path="hardware/xiaomi" name="platform_hardware_xiaomi" remote="streak-sc" />

  <!-- packages -->
  <project path="packages/apps/Launcher3" name="platform_packages_apps_Launcher3" remote="streak-sc" />
  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="streak-sc" />
  <project path="packages/apps/SettingsIntelligence" name="platform_packages_apps_SettingsIntelligence" remote="streak-sc" />
  <project path="packages/apps/ThemePicker" name="platform_packages_apps_ThemePicker" remote="streak-sc" />
  <project path="packages/apps/WallpaperPicker2" name="platform_packages_apps_WallpaperPicker2" remote="streak-sc" />
  <project path="packages/modules/adb" name="platform_packages_modules_adb" groups="pdk" remote="streak-sc" />
  <project path="packages/services/Telephony" name="platform_packages_services_Telephony" remote="streak-sc" />

  <!-- prebuilts -->
  <project path="prebuilts/tools-streak" name="platform_prebuilts_tools-streak" remote="streak-sc" />

  <!-- system -->
  <project path="system/core" name="platform_system_core" remote="streak-sc" />
  <project path="system/security" name="platform_system_security" remote="streak-sc" />
  <project path="system/sepolicy" name="platform_system_sepolicy" remote="streak-sc" />
  <project path="system/vold" name="platform_system_vold" remote="streak-sc" />

  <!-- vendor -->
  <project path="vendor/streak" name="platform_vendor_streak" remote="streak-sc" />

</manifest>
